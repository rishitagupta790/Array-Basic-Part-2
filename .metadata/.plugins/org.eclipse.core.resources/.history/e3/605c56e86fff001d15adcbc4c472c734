package com.in;

import java.util.Arrays;

class Multiple {
	int mul(int arr[]) {
		int mul = 1;
		Arrays.sort(arr);
		for (int i = arr.length - 1; i > 0; i--) {
			mul = arr[i] * mul;
		}
		return 0;
	}
}

public class Array_04 {

	public static void main(String[] args) {
		Multiple call = new Multiple();
		int array[] = { 2, 5, 7, 8, 8 };
		call.mul(array);
	}
}
